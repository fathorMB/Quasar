<div class="workspace__placeholder">
    <p class="workspace__eyebrow">@Title</p>
    <p>@Message</p>
</div>

@code {
    [Parameter]
    public string Title { get; set; } = "Workspace";

    [Parameter]
    public string Message { get; set; } = "Use AddQuasarUi(...) to register a custom component for this menu item.";
}
